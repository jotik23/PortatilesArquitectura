<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="Mosaddek">
    <meta name="keyword" content="FlatLab, Dashboard, Bootstrap, Admin, Template, Theme, Responsive, Fluid, Retina">
    <link rel="shortcut icon" href="img/favicon.html">

    <title>Portatiles</title>

    <!-- Bootstrap core CSS -->
    <link href="http://localhost:3000/css/bootstrap.min.css" rel="stylesheet">
    <link href="http://localhost:3000/css/bootstrap-reset.css" rel="stylesheet">
    <!--external css-->
    <link href="http://localhost:3000/assets/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link href="http://localhost:3000/assets/jquery-easy-pie-chart/jquery.easy-pie-chart.css" rel="stylesheet" type="text/css" media="screen"/>
    <link rel="stylesheet" href="http://localhost:3000/css/owl.carousel.css" type="text/css">
    <!-- Custom styles for this template -->
    <link href="http://localhost:3000/css/style.css" rel="stylesheet">
    <link href="http://localhost:3000/css/style-responsive.css" rel="stylesheet" />

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 tooltipss and media queries -->
    <!--[if lt IE 9]>
      <script src="http://localhost:3000/js/html5shiv.js"></script>
      <script src="http://localhost:3000/js/respond.min.js"></script>
    <![endif]-->
  </head>

  <body>

  <section id="container" class="">
      <!--header start-->
      <header class="header white-bg">
            <div class="sidebar-toggle-box">
                <div data-original-title="Toggle Navigation" data-placement="right" class="icon-reorder tooltips"></div>
            </div>
            <!--logo start-->
            <a href="#" class="logo">Flat<span>lab</span></a>
            <!--logo end-->
            <div class="nav notify-row" id="top_menu">
                
            </div>
            <div class="top-nav ">
                <!--search & user info start-->
                <ul class="nav pull-right top-menu">
                    <li>
                        <input type="text" class="form-control search" placeholder="Search">
                    </li>
                    <!-- user login dropdown start-->
                    <li class="dropdown">
                        <a data-toggle="dropdown" class="dropdown-toggle" href="#">
                            <img alt="" src="http://localhost:3000/img/avatar1_small.jpg">
                            <span class="username">Jhon Jevis</span>
                            <b class="caret"></b>
                        </a>
                        <ul class="dropdown-menu extended logout">
                            <div class="log-arrow-up"></div>
                            <li><a href="login.html"><i class="icon-key"></i> Salir</a></li>
                        </ul>
                    </li>
                    <!-- user login dropdown end -->
                </ul>
                <!--search & user info end-->
            </div>
        </header>
      <!--header end-->
      <!--sidebar start-->
      <aside>
          <div id="sidebar"  class="nav-collapse ">
              <!-- sidebar menu start-->
              <ul class="sidebar-menu">
                  <li class="active">
                      <a class="" href="/registros">
                          <i class="icon-dashboard"></i>
                          <span>Principal</span>
                      </a>
                  </li>
                  <li class="sub-menu">
                      <a href="http://localhost:3000/perfils" class="">
                          <i class="icon-book"></i>
                          <span>Perfiles</span>
                      </a>
                  </li>
                  <li class="sub-menu">
                      <a href="http://localhost:3000/dispositivos" class="">
                          <i class="icon-cogs"></i>
                          <span>Dispositivos</span>
                      </a>
                  </li>
                  <li class="sub-menu">
                      <a href="http://localhost:3000/sedes" class="">
                          <i class="icon-tasks"></i>
                          <span>Sedes</span>
                      </a>
                  </li>
                  <li class="sub-menu">
                      <a href="http://localhost:3000/personas" class="">
                          <i class="icon-th"></i>
                          <span>Personas</span>
                      </a>
                  </li>
                  
              </ul>
              <!-- sidebar menu end-->
          </div>
      </aside>
      <!--sidebar end-->
      <!--main content start-->
      <section id="main-content">
          <section class="wrapper">
              <%= yield %>
          </section>
      </section>
      <!--main content end-->
  </section>

    <!-- js placed at the end of the document so the pages load faster -->
    <script src="http://localhost:3000/js/jquery.js"></script>
    <script src="http://localhost:3000/js/jquery-1.8.3.min.js"></script>
    <script src="http://localhost:3000/js/bootstrap.min.js"></script>
    <script src="http://localhost:3000/js/jquery.scrollTo.min.js"></script>
    <script src="http://localhost:3000/js/jquery.nicescroll.js" type="text/javascript"></script>
    <script src="http://localhost:3000/js/jquery.sparkline.js" type="text/javascript"></script>
    <script src="http://localhost:3000/assets/jquery-easy-pie-chart/jquery.easy-pie-chart.js"></script>
    <script src="http://localhost:3000/js/owl.carousel.js" ></script>
    <script src="http://localhost:3000/js/jquery.customSelect.min.js" ></script>

    <!--common script for all pages-->
    <script src="http://localhost:3000/js/common-scripts.js"></script>

    <!--script for this page-->
    <script src="http://localhost:3000/js/sparkline-chart.js"></script>
    <script src="http://localhost:3000/js/easy-pie-chart.js"></script>

  <script>

      //owl carousel

      $(document).ready(function() {
          $("#owl-demo").owlCarousel({
              navigation : true,
              slideSpeed : 300,
              paginationSpeed : 400,
              singleItem : true

          });
      });

      //custom select box

      $(function(){
          $('select.styled').customSelect();
      });

  </script>

  </body>
</html>

